{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "UI Dev",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev"]
    },
    {
      "name": "UI Build",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["build"]
    },
    {
      "name": "UI Preview",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["preview"]
    },
    {
      "name": "UI Generate CSP Script Hashes",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["generate:csp-script-hashes"]
    },
    {
      "name": "UI Fix jsdom",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["fix:jsdom"]
    },
    {
      "name": "Checks",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/checks",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["test:debug"]
    },
    {
      "name": "Storybook",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/ui",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["storybook"]
    }
  ]
}
